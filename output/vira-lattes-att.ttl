:Monografia a gufo:SubKind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Monografia"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Avaliacao ;
            owl:onProperty gufo:participatedIn ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :Publicacao .

:Ata a gufo:Kind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Ata"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Avaliacao ;
            owl:onProperty [ owl:inverseOf gufo:inheresIn ] ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        gufo:IntrinsicMode .

:Avaliacao a gufo:EventType,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Avaliacao"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Ata ;
            owl:onProperty gufo:inheresIn ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Monografia ;
            owl:onProperty [ owl:inverseOf gufo:participatedIn ] ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Banca ;
            owl:onProperty [ owl:inverseOf gufo:participatedIn ] ],
        gufo:Event .

:Publicacao a gufo:Kind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Publicacao"@en ;
    rdfs:subClassOf gufo:FunctionalComplex .

:Banca a gufo:Kind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Banca"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Avaliacao ;
            owl:onProperty gufo:participatedIn ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        gufo:FunctionalComplex .

:Pesquisador a gufo:Kind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Pesquisador"@en ;
    rdfs:subClassOf gufo:FunctionalComplex .


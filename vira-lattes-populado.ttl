:Curso a gufo:Kind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Curso"@en ;
    rdfs:subClassOf gufo:FunctionalComplex .

:Instituicao a gufo:Kind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Instituicao"@en ;
    rdfs:subClassOf gufo:FunctionalComplex .

:Avaliador a gufo:Role,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Avaliador"@en ;
    rdfs:subClassOf :Pesquisador .

:Monografia a gufo:SubKind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Monografia"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Avaliacao ;
            owl:onProperty gufo:participatedIn ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :Publicacao .

:Ata a gufo:Kind,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Ata"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass :Avaliacao ;
            owl:onProperty [ owl:inverseOf gufo:inheresIn ] ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        gufo:IntrinsicMode .

:Orientador a gufo:Role,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Orientador"@en ;
    rdfs:subClassOf :Pesquisador .

:Orientando a gufo:Role,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Orientando"@en ;
    rdfs:subClassOf :Pesquisador .

:Avaliacao a gufo:EventType,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:label "Avaliacao"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Ata ;
            owl:onProperty gufo:inheresIn ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Monografia ;
            owl:onProperty [ owl:inverseOf gufo:participatedIn ] ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass :Banca ;
            owl:onProperty [ owl:inverseOf gufo:participatedIn ] ],
        gufo:Event .

